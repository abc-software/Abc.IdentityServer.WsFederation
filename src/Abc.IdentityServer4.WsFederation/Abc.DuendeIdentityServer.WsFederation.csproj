<Project Sdk="Microsoft.NET.Sdk">
  <!--<PropertyGroup>
    <BaseIntermediateOutputPath>obj\DuendeIDS\</BaseIntermediateOutputPath>
    <OutputPath>bin\DuendeIDS\</OutputPath>
  </PropertyGroup>-->
  <ItemGroup>
    <Compile Remove="obj\**" />
    <EmbeddedResource Remove="obj\**" />
    <None Remove="obj\**" />
  </ItemGroup>
  
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DefineConstants>$(DefineConstants);DUENDE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup>
    <Description>WS-Federation protocol for Duende IdentityServer</Description>
  </PropertyGroup>

  <ItemGroup>
      <None Include="../../LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
   <PackageReference Include="Duende.IdentityServer" Version="5.2.*" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.IdentityModel.Protocols.WsFederation" Version="6.31.0" />
    <PackageReference Include="Microsoft.IdentityModel.Logging" Version="6.31.0" />
    <PackageReference Include="Microsoft.IdentityModel.Protocols" Version="6.31.0" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.31.0" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens.Saml" Version="6.31.0" />
    <PackageReference Include="Microsoft.IdentityModel.Xml" Version="6.31.0" />
  </ItemGroup>
  
  <ItemGroup>
    <InternalsVisibleTo Include="Abc.DuendeIdentityServer.WsFederation.UnitTests, " />
    <InternalsVisibleTo Include="Abc.DuendeIdentityServer.WsFederation.IntegrationTests, " />
  </ItemGroup>

</Project>
